<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <resource-ref>
    <description>Connection</description>
    <res-ref-name>jdbc/mysql</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
  <display-name>maker_space</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>BoardServiceController</servlet-name>
    <servlet-class>work.controller.BoardServiceController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BoardServiceController</servlet-name>
    <url-pattern>/boardcontroller</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>MemberServiceController</servlet-name>
    <servlet-class>work.controller.MemberServiceController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MemberServiceController</servlet-name>
    <url-pattern>/membercontroller</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>work.controller.InitListener</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>TipBoardServiceController</display-name>
    <servlet-name>TipBoardServiceController</servlet-name>
    <servlet-class>work.controller.TipBoardServiceController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TipBoardServiceController</servlet-name>
    <url-pattern>/tipboardcontroller</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ProcessServiceController</display-name>
    <servlet-name>ProcessServiceController</servlet-name>
    <servlet-class>work.controller.ProcessServiceController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ProcessServiceController</servlet-name>
    <url-pattern>/processcontroller</url-pattern>
  </servlet-mapping>
</web-app>